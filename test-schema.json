{
	"$version": "0.1",
	"$paths": {
		"bill-text": "body.div[id=main-block].main.div[id=billText]"
	},
	"title": {
		"$type": "string",
		"$selector": "body.div[id=main-block].main.h3"
	},
	"bill_number": {
		"$type": "number",
		"$selector": "body.div[id=main-block].main.div[id=billText].div[id=substrdiv].h2"
	},
	"sections": {
		"$type": "array",
		"$itr_el": "body.div.main.section.table.tbody",
		"$itr_el_match": "tr",
		"$itr_ident": "table-row",
		"$start_offset": 1,
		"$item": {
			"title": {
				"$type": "string",
				"$selector": "@table-row.td.div.h3"
			},
			"content": {
				"$type": "string",
				"$selector": "@table-row.td.div.p[nth=1]"
			},
			"bill_numbers": {
				"$type": "array",
				"$itr_el": "@table-row.td.div.div",
				"$itr_el_match": "*",
				"$itr_ident": "sub-section",
				"$item": {
					"$type": "number",
					"$selector": "@sub-section.p[id=bill-number]"
				}
			}
		}
	},
	"link": {
		"$type": "object",
		"$key_node": {
			"$type": "string",
			"$selector": "body.div[id=main-block].main.section[id=contact-info].div[id=name]"
		},
		"$value_node": {
			"$type": "string",
			"$selector": "a:href"
		}
	},
	"links": {
		"$type": "object",
		"$itr_node": "body.div.main.section",
		"$itr_node_match": "div",
		"$itr_ident": "contact-info",
		"$key_node": {
			"$type": "string",
			"$selector": "@contact-info.div[id=name]"
		},
		"$value_node": {
			"$type": "string",
			"$selector": "@contact-info.a:href"
		}
	}
}
