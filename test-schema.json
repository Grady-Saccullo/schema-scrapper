{
	"$version": "0.1",
	"title": {
		"$type": "string",
		"$selector": "body.div[id=main-block].main.div[id=billStatus].span"
	},
	"test": {
		"$type": "string",
		"$selector": "body.div[id=main-block].main.div[!nth=2].span"
	},
	"bill_number": {
		"$type": "number",
		"$selector": "body.div[id=main-block].main.div[id=billText].div[id=substrdiv].h2"
	},
	"sections": {
		"$type": "array",
		"$itr_el": "body.div[id=main-block].main.div[id=billStatus].div.table.tbody",
		"$itr_el_match": "tr",
		"$itr_ident": "table-row",
		"$start_offset": 1,
		"$value_node": {
			"date": {
				"$type": "string",
				"$selector": "@table-row.td[!nth=0].font"
			},
			"action": {
				"$type": "string",
				"$selector": "@table-row.td[!nth=1].font"
			}
		}
	},
	"link": {
		"$type": "object",
		"$key_node": {
			"$type": "string",
			"$selector": "body.div[id=main-block].main.div[id=billStatus].span"
		},
		"$value_node": {
			"$type": "string",
			"$selector": "body.div[id=main-block].main.div[id=billStatus].span"
		}
	},
	"links": {
		"$type": "object",
		"$itr_el": "body.div.main.section",
		"$itr_el_match": "div",
		"$itr_ident": "table-row",
		"$key_node": {
			"$type": "string",
			"$selector": "@contact-info.div[id=name]"
		},
		"$value_node": {
			"$type": "string",
			"$selector": "@contact-info.a:href"
		}
	}
}
